# aliases
alias aj="autojump"
alias bu="brew update && brew upgrade && brew prune && brew doctor"
alias bear="open /Applications/Bear.app"
alias bye="exit"
alias c="clear"
alias clear="clear && quoth"
alias db="dotnet build"
alias decode3p="node ~/.nvm/versions/node/v8.5.0/lib/node_modules/third-party-decode/index.js"
alias dr="dotnet run"
alias e="exit"
alias g="googler"
alias gc="git commit -am"
alias gca="git commit --amend"
alias gl="git log --graph --pretty=format:'%Cred%h%Creset%n' --abbrev-commit --date=relative --branches --all"
alias ls="ls -la"
alias q="quoth"
alias nl="npm list --depth=0"
alias ns="npm start"
alias ngo="npm run lint && npm test && npm start"
alias urldecode='python -c "import sys, urllib as ul; print ul.unquote_plus(sys.argv[1])"'
alias urlencode='python -c "import sys, urllib as ul; print ul.quote_plus(sys.argv[1])"'

# aliases for programs
alias 1password="open -a 1password"
alias chrome="open -a google\ chrome"
alias contacts="open -a contacts"
alias outlook="open -a microsoft\ outlook"
alias safari="open -a safari"
alias tidal="open -a tidal"
alias web="safari"
